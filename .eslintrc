{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "extends": [
    "problems",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "node": true,
    "es6" : true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "flowVersion": "0.80"
    }
  },
  "rules": {
    "no-duplicate-imports": "off"
  }
}
